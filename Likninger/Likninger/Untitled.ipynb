   "source": [
    "def f(x):\n",
    "    return x-1\n",
    "\n",
    "a = -5\n",
    "b =5\n",
    "m = (a+b)/2\n",
    "\n",
    "while f(m) != 0:\n",
    "    if f(a)*f(m) < 0:\n",
    "        b = m\n",
    "    elif f(b)*f(m) < 0:\n",
    "        a = m\n",
    "    m = (a+b)/2\n",
    "print(m)\n"
   ]
